getData <- function(fileURL, destfile){
if(!file.exists("data"))
dir.create("data")
download.file(fileURL, destfile)
data <- read.csv(destfile, na.strings = c("(millions of", "US dollars)", "", ".."), stringsAsFactors = FALSE)
data
}
